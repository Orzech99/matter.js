{
    "name": "@project-chip/matter.js-react-native-ble",
    "version": "0.8.0-alpha.0-20240319-3d88df96",
    "description": "Matter BLE support for node.js",
    "keywords": [
        "iot",
        "home automation",
        "matter",
        "smart device",
        "ble"
    ],
    "license": "Apache-2.0",
    "author": "Project CHIP Authors",
    "contributors": [
        "Ingo Fischer <github@fischer-ka.de>"
    ],
    "bugs": {
        "url": "https://github.com/project-chip/matter.js/issues"
    },
    "homepage": "https://github.com/project-chip/matter.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/project-chip/matter.js.git"
    },
    "scripts": {
        "clean": "matter-build clean",
        "build": "matter-build",
        "build-clean": "matter-build --clean",
        "build-doc": "typedoc --excludeExternals --plugin typedoc-plugin-missing-exports --plugin typedoc-plugin-markdown --tsconfig src/tsconfig.json"
    },
    "devDependencies": {
        "@project-chip/matter.js-tools": "0.10.0-alpha.0-20240611-823c6da5",
        "ts-node": "^10.9.2",
        "typescript": "~5.4.5"
    },
    "dependencies": {
        "@project-chip/matter.js": "0.10.0-alpha.0-20240611-823c6da5",
        "react-native-ble-plx": "^3.2.0"
    },
    "files": [
        "dist/**/*",
        "src/**/*",
        "require/**/*",
        "LICENSE",
        "README.md"
    ],
    "type": "module",
    "main": "dist/cjs/ble/export.js",
    "types": "dist/cjs/ble/export.d.ts",
    "exports": {
        "./package.json": "./package.json",
        "./*": {
            "import": {
                "types": "./dist/esm/*/export.d.ts",
                "default": "./dist/esm/*/export.js"
            }
        }
    },
    "typesVersions": {
        "*": {
            ".": [
                "/dist/cjs/ble/export.d.ts"
            ],
            "*": [
                "/dist/cjs/*/export.d.ts"
            ]
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
