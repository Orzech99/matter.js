{
    "name": "@project-chip/matter.js-jscrypto",
    "version": "0.10.0-alpha.0-20240615-fe1cf4ba",
    "description": "Matter.js crypto implementation using pure JavaScript",
    "keywords": [
        "iot",
        "home automation",
        "matter",
        "smart device"
    ],
    "license": "Apache-2.0",
    "author": "Project CHIP Authors",
    "contributors": [
        "Ingo Fischer <github@fischer-ka.de>"
    ],
    "bugs": {
        "url": "https://github.com/project-chip/matter.js/issues"
    },
    "homepage": "https://github.com/project-chip/matter.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/project-chip/matter.js.git"
    },
    "scripts": {
        "clean": "matter-build clean",
        "build": "matter-build",
        "build-clean": "matter-build --clean",
        "build-doc": "typedoc --excludeExternals --plugin typedoc-plugin-missing-exports --plugin typedoc-plugin-markdown --tsconfig src/tsconfig.docs.json",
        "test": "matter-test -w",
        "coverage": "c8 matter-test esm"
    },
    "dependencies": {
        "@project-chip/matter.js": "0.10.0-alpha.0-20240615-fe1cf4ba",
        "crypto-browserify": "^3.12.0"
    },
    "overrides": {
        "browserify-aes": "npm:@matter.js/browserify-aes@1.3.0"
    },
    "devDependencies": {
        "@project-chip/matter.js-tools": "0.10.0-alpha.0-20240615-fe1cf4ba",
        "@types/chai": "^4.3.12",
        "@types/mocha": "^10.0.6",
        "@types/wtfnode": "^0.7.3",
        "embedme": "^1.22.1",
        "chai": "^4.4.1",
        "mocha": "^10.4.0",
        "typescript": "~5.4.5",
        "events": "^3.3.0",
        "stream": "^0.0.2"
    },
    "files": [
        "dist/**/*",
        "src/**/*",
        "LICENSE",
        "README.md"
    ],
    "type": "module",
    "main": "dist/cjs/export.js",
    "module": "dist/esm/export.js",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/export.d.ts",
                "default": "./dist/esm/export.js"
            },
            "require": {
                "types": "./dist/cjs/export.d.ts",
                "default": "./dist/cjs/export.js"
            }
        },
        "./package.json": "./package.json",
        "./*": {
            "import": {
                "types": "./dist/esm/*/export.d.ts",
                "default": "./dist/esm/*/export.js"
            },
            "require": {
                "types": "./dist/cjs/*/export.d.ts",
                "default": "./dist/cjs/*/export.js"
            }
        }
    },
    "types": "dist/esm/export.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "/dist/cjs/export.d.ts"
            ],
            "*": [
                "/dist/cjs/*/export.d.ts"
            ]
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
